      <abstract id="abstract-1">
        <p id="p-1">Hi there. This is an experiment of collaboratively editing a reproducible document. Please join the fun!</p>
      </abstract>
      <trans-abstract id="trans-abstract-1" xml:lang="de">
        <p id="p-2">Deutsche Übersetzung</p>
      </trans-abstract>
